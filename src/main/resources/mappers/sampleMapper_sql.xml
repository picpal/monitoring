<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.picpal.framework.sample.mapper.SampleMapper">
    <!-- ####### user insert   -->
    <insert id="insertUserRecord">
        insert into SAMPLE_ENTITY(
            id,
            age,
            birthday ,
            name,
            username
        ) values (
            #{id},
            #{age},
            #{birthday},
            #{name},
            #{username}
        )
    </insert>

    <!-- ####### coupon insert   -->
    <insert id="insertExcelCells">
        insert into excel_cells(
            col1,
            col2,
            col3,
            col4,
            col5,
            col6,
            col7
        )values(
            #{col1},
            #{col2},
            #{col3},
            #{col4},
            #{col5},
            #{col6},
            #{col7}
        )
    </insert>
</mapper>
